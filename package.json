{
  "name": "todo-list",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.6.0",
  "scripts": {
    "dev:front": "vite",
    "dev:back": "cd D:\\code\\symfony\\todolistAPI && symfony server:start",
    "dev": "concurrently \"npm run dev:front\" \"npm run dev:back",
